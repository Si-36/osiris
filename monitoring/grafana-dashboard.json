{
  "dashboard": {
    "id": null,
    "title": "AURA Intelligence Production Dashboard",
    "tags": ["aura", "production", "ai"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "aura_system_health",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "GPU Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "aura_gpu_processing_time_ms",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Milliseconds", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Request Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aura_requests_total[5m])",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "aura_memory_usage_bytes",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Bytes", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Neural Architecture Performance",
        "type": "table",
        "targets": [
          {
            "expr": "aura_neural_architecture_score",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "System Alerts",
        "type": "alertlist",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}