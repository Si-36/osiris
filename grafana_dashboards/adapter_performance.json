        "w": 24,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum by (adapter) (rate(aura_adapter_throughput_ops_per_second[5m]))",
          "legendFormat": "{{adapter}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "ops",
