      "id": 11,
      "type": "stat",
      "title": "Memory Gpu Speedup",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "aura_gpu_adapter_speedup{adapter=\"memory_gpu\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 2
              },
              {
                "color": "green",
                "value": 10
              }
            ]
          },
          "unit": "none",
          "custom": {
            "postfix": "x"
          }
