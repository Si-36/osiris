{
  "uid": "aura-adapter-performance",
  "title": "AURA Adapter Performance",
  "tags": [
    "adapters",
    "performance",
    "gpu",
    "aura"
  ],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
