      "title": "Adapter Latency Comparison",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (adapter, le) (rate(aura_component_latency_seconds_bucket[5m])))",
